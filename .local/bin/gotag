#!/bin/bash
# This script needs "xdo" to work
# Please install them before trying to run it
# 

case $1 in
	t1)
		xdo key_press -k 133
		xdo key_press -k 10 
		xdo key_release -k 133 
		xdo key_release -k 10
		;;
	t2)
	 	xdo key_press -k 133
		xdo key_press -k 11
		xdo key_release -k 133
		xdo key_release -k 11
		;;
	t3)
		xdo key_press -k 133
		xdo key_press -k 12
		xdo key_release -k 133
		xdo key_release -k 12
		;;
	t4)
		xdo key_press -k 133
		xdo key_press -k 13
		xdo key_release -k 133
		xdo key_release -k 13
		;;
	t5)
		xdo key_press -k 133
		xdo key_press -k 14
		xdo key_release -k 133
		xdo key_release -k 14
		;;
	t6)
		xdo key_press -k 133
		xdo key_press -k 14
		xdo key_release -k 133
		xdo key_release -k 14
		;;	
	t1)
		xdo key_press -k 133
		xdo key_press -k 16
		xdo key_release -k 133
		xdo key_release -k 16
		;;
esac